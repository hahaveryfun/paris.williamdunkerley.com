{
	"name": "fronttest",
	"license": "MIT",
	"scripts": {
		"start": "cross-env API_SUBDOMAIN=prague parcel index.html ",
		"build:dev": "cross-env API_SUBDOMAIN=prague parcel build index.html --no-minify --public-url ./",
		"build:qa": "cross-env API_SUBDOMAIN=santiago parcel build index.html --no-minify --public-url ./",
		"build:prod": "cross-env API_SUBDOMAIN=amsterdam parcel build index.html --no-minify --public-url ./",
		"build": "build:prod"
	},
	"dependencies": {
		"@material-ui/core": "^4.9.5",
		"autoprefixer": "^7.2.1",
		"axios": "^0.19.2",
		"babel-preset-react-app": "^3.1.0",
		"dotenv": "^8.2.0",
		"html-react-parser": "^0.10.3",
		"parcel-bundler": "latest",
		"parse5": "^5.1.1",
		"react": "^16.2.0",
		"react-dom": "^16.2.0",
		"react-redux": "^7.2.0",
		"redux": "^4.0.5",
		"redux-thunk": "^2.3.0",
		"rss-parser": "^3.7.6"
	},
	"postcss": {
		"modules": false,
		"plugins": {
			"autoprefixer": {
				"browsers": [
					">1%",
					"last 4 versions",
					"Firefox ESR",
					"not ie < 9"
				],
				"flexbox": "no-2009"
			}
		}
	},
	"devDependencies": {
		"babel-core": "^6.26.3",
		"cross-env": "^5.1.1"
	}
}
